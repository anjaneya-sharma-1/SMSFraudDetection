{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    }
  ],
  "functions": {
    "app/api/analyze/route.ts": {
      "maxDuration": 30
    },
    "app/api/feedback/route.ts": {
      "maxDuration": 10
    }
  }
}